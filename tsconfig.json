{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES6",
    "lib": [
      "es2015",
      "dom"
    ],
    "moduleResolution": "node",
    "baseUrl": "./src",
    "outDir": "dist/tsc-compiled",
    "typeRoots": [
      "node_modules/@types"
    ],
    "esModuleInterop": true,
    "allowJs": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*.ts"
  ]
}
